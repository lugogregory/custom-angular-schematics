{
  "name": "custom-angular-schematic-lib",
  "version": "0.0.8",
  "scripts": {
    "build": "npm install && ng build && tsc -p tsconfig.schematics.json",
    "postbuild": "copyfiles schematics/*/schema.json schematics/*/files/** schematics/collection.json ../../dist/custom-angular-schematic-lib/"
  },
  "schematics": "./schematics/collection.json",
   "ng-add": {
    "save": "devDependencies"
  },
  "peerDependencies": {
    "@angular/common": "^19.2.0",
    "@angular/core": "^19.2.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
    "devDependencies": {
    "copyfiles": "^2.4.1",
    "typescript": "~5.7.2"
  },
  "sideEffects": false
}
